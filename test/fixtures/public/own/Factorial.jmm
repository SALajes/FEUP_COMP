import static ioPlus.printResult(int) void;

class Factorial{
    public static void main(String[] args) {
        Factorial fac;

        fac = new Factorial();

        ioPlus.printResult(fac.factorial(9));
    }

    public int factorial(int c){
        int result;
        result = 1;

        while(0 < c){
            result = result * c;
            c = c - 1;
        }

        return result;
    }
}